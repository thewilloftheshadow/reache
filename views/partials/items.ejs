<div class="center padtop storediv">
  <div class="row row-cols-1 row-cols-lg-5 row-cols-md-3 row-cols-sm-2">
    <% items.forEach(x => { %>
      <div class="col mb-4">
        <div class="card text-center h-250">
          <img src="<%- x.image %>" class="card-img-top" alt="<%- x.name %>">
          <div class="card-body">
            <h5 class="card-title"><%- x.name %></h5>
            <p class="card-text"><%- x.description || "" %></p>
            <button class="snipcart-add-item btn btn-success" data-item-id="<%- x.id %>" data-item-price="<%- x.price %>" data-item-url="<%- x.url %>" data-item-description="<%- x.description || "" %>"   data-item-image="<%- x.image %>" data-item-name="<%- x.name %>" data-item-custom1-name="Size" data-item-custom1-options="<%- x.sizefield %>">Add to cart</button>
          </div>
        </div>
      </div>
    <% }) %>
  </div>